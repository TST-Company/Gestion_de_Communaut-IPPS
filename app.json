{
  "expo": {
    "name": "gestion-de-communaute",
    "slug": "gestion-de-communaute",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo_ipps.png",
    "scheme": "gestiondecommunaute",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash-logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ECE8D8"
    },
    "ios": {
      "supportsTablet": true,
      "splash": {
        "image": "./assets/images/splash-logo.png",
        "resizeMode": "contain",
        "backgroundColor": "#ECE8D8"
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/splash-logo.png",
        "backgroundImage": "./assets/images/splash-logo.png",
        "monochromeImage": "./assets/images/splash-logo.png"
      },
      "splash": {
        "image": "./assets/images/splash-logo.png",
        "resizeMode": "contain",
        "backgroundColor": "#ECE8D8"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/logo_ipps.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ECE8D8"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "assetBundlePatterns": [
      "**/*"
    ]
  }
}